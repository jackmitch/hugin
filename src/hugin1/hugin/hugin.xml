<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-ptoptimizer-script">
    <sub-class-of type="text/plain"/>
    <comment xml:lang="en">Panorama project for Hugin</comment>
    <comment xml:lang="cs">Skript pro panorama pro Hugin</comment>
    <magic priority="99">
        <match type="string" value="# hugin project file" offset="0"/>
    </magic>
    <glob pattern="*.pto" />
  </mime-type>
</mime-info>
